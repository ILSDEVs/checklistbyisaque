{
  "appId": "com.checklistbyisaque.app",
  "productName": "Checklist By Isaque",
  "directories": {
    "output": "dist-electron"
  },
  "files": [
    "dist/**/*",
    "electron.js",
    "package.json"
  ],
  "win": {
    "target": [
      {
        "target": "portable",
        "arch": ["x64"]
      }
    ],
    "icon": "public/favicon.ico"
  },
  "portable": {
    "artifactName": "ChecklistByIsaque-Portable.exe"
  },
  "mac": {
    "target": "dmg",
    "icon": "public/favicon.ico"
  },
  "linux": {
    "target": "AppImage",
    "icon": "public/favicon.ico"
  }
}